[run]
parallel = true
source =
    pyramid_openapi3
;branch = True

[paths]
source =
    pyramid_openapi3
    */site-packages/pyramid_openapi3

[report]
show_missing = true
precision = 2

# Regexes for lines to exclude from consideration
;exclude_lines =
;    # Have to re-enable the standard pragma
;    pragma\: no cover
;
;    # Don't complain if non-runnable code isn't run:
;    if __name__ == "__main__"\:
